/boot		emmc	/dev/block/bootdevice/by-name/BOOT	flags=display="boot";backup=1;flashimg
/recovery	emmc	/dev/block/bootdevice/by-name/RECOVERY	flags=display="recovery";backup=1;flashimg
/system		ext4	/dev/block/bootdevice/by-name/SYSTEM	flags=display="system";backup=1;flashimg
/vendor		ext4	/dev/block/bootdevice/by-name/VENDOR	flags=display="vendor";backup=1;flashimg
/data		ext4	/dev/block/bootdevice/by-name/USERDATA	flags=encryptable=footer;length=-20480;display="data";backup=1;flashimg
/cache		ext4	/dev/block/bootdevice/by-name/CACHE	flags=display="cache";backup=1;flashimg
/preload	ext4	/dev/block/bootdevice/by-name/HIDDEN	flags=display="preload";backup=1;flashimg
/odm		ext4	/dev/block/bootdevice/by-name/ODM	flags=display="omc";backup=1;flashimg
/modem		emmc	/dev/block/bootdevice/by-name/RADIO	flags=display="Modem";backup=1
/efs		ext4	/dev/block/bootdevice/by-name/EFS	flags=display="EFS";backup=1
/external_sd	vfat	/dev/block/mmcblk0p1 /dev/block/mmcblk0	flags=display="Micro SDcard";storage;wipeingui;removable
/usb-otg	vfat	/dev/block/sdf1 /dev/block/sdf		flags=display="USB-OTG";storage;wipeingui;removable
